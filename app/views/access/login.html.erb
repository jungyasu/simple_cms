<% @page_title = "Admin Login" %>

<div class="login center">
	<%= form_tag(:action => 'attempt_login') do %>
	
			<p><%= label_tag(:username) %></p>
			<p><%= text_field_tag(:username) %></p>
		
			<p><%= label_tag(:password) %></p>
			<p><%= password_field_tag(:password) %></p>
		
			<p><%= submit_tag("Log In") %></p>
		
	<% end %>
</div>
